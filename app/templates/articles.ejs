<!DOCTYPE html>
<html lang="ko">

<head>
  <meta charset="UTF-8" />
  <title>Park Seong-beom</title>

  <meta name="viewport" content="width=device-width, user-scalable=no">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <meta name="theme-color" content="#F5F5F5">
  <link rel="canonical" href="https://parksb.github.io/articles.html" />

  <meta property="og:title" content="Park Seong-beom">
  <meta property="og:image" content="https://user-images.githubusercontent.com/6410412/55271766-bf369000-52f5-11e9-98fe-ecdef51e1633.png">
  <meta property="og:description" content="박성범 (Park Seong-beom)">

  <meta name="twitter:title" content="Park Seong-beom">
  <meta name="twitter:description" content="박성범 (Park Seong-beom)">
  <meta name="twitter:image" content="https://user-images.githubusercontent.com/6410412/55271766-bf369000-52f5-11e9-98fe-ecdef51e1633.png">
  <meta name="twitter:card" content="summary_large_image">

  <link rel="preload" href="../src/scss/init.scss" as="style">
  <link rel="preload" href="../src/scss/articles.scss" as="style">

  <link rel="shortcut icon" href="../assets/favicon.ico" type="image/x-icon">
  <link rel="stylesheet" href="../src/scss/init.scss">
  <link rel="stylesheet" href="../src/scss/articles.scss">

  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-113147800-1"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());
    gtag('config', 'UA-113147800-1');
  </script>
</head>

<body>
  <header id="top-container" role="navigation">
  </header>

  <main id="main-container">
    <section id="articles-container">
      <ul>
        <% articles.reverse().map((article) => { %>
          <li id="articles-list-item">
            <a href="./article/<%= article.id %>.html">
              <h1><%= article.title %></h1>
              <% if (article.subtitle) { %>
                <h2><%= article.subtitle %></h2>
              <% } %>
              <time><%= article.date %></time>
              <ul class="articles-tags">
                <% article.tags.map((tag) => { %>
                <li>#<%= tag.trim() %></li>
                <% }) %>
              </ul>
            </a>
          </li>
        <% }) %>
      </ul>
    </section>
  </main>

  <script defer src="../src/ts/init.ts"></script>
</body>

</html>
